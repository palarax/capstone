{
    "logger": {
        "logger": "trainer",
        "filename": "logs/test.log",
        "level": "DEBUG",
        "format": "%(asctime)s %(levelname)s:%(message)s",
        "datefmt": "%Y-%m-%d %H:%M:%S"
    },

    "model" : {
        "min_input_size":       288,
        "max_input_size":       448,
        "anchors1":              [23,93, 47,171, 65,277, 92,345, 96,199, 114,262, 134,365, 186,365, 261,354],
        "anchors":               [13,55, 34,118, 51,217, 73,299, 84,204, 110,362, 112,270, 173,349, 278,362],
        "labels1":               ["pedestrian", "cyclist", "on-call", "on-mobile"],
        "labels":               ["background", "cyclist", "risk"],
        "weights":              "configuration/yolov3.h5"
    },

    "train": {
        "train_image_folder":   "dataset/images/train",
        "annotations":          "dataset/annotations.txt",
        "batch_size":           8,
        "learning_rate":        1e-4,
        "epochs":               10,

        "tensorboard_dir":      "logs/tensorboard/000",
        "model_stages":         "model_stages/000/",
        "debug": false  
    }

}
